		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="GenericParam">
		<xs:sequence>
			<xs:element name="paramKey" type="xs:string"/>
			<xs:element name="paramValue" type="xs:string"/>
		</xs:sequence>
	</xs:complexType>
